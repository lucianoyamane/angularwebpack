{
  "name": "angular-webpack",
  "version": "0.0.1",
  "description": "Angular application with webpack",
  "scripts": {
    "webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
    "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "webpack serve --inline --hot --progress --port 8080",
    "build:prod": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
    "build:clean": "del-cli 'src/**/*.js' 'src/**/*.js.map' 'src/**/*.ngsummary.json' 'src/**/*.metadata.json' 'src/**/**/*.ngfactory.ts' 'src/**/*.ngstyle.ts' 'src/**/*.shim.ts'",
    "serve": "lite-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucianoyamane/angularwebpack.git"
  },
  "author": "luciano yamane",
  "license": "MIT",
  "homepage": "https://github.com/lucianoyamane/angularwebpack.git",
  "dependencies": {
    "@angular/animations": "~12.2",
    "@angular/common": "~12.2",
    "@angular/compiler": "~12.2",
    "@angular/compiler-cli": "~12.2",
    "@angular/core": "~12.2",
    "@angular/forms": "~12.2",
    "@angular/platform-browser": "~12.2",
    "@angular/platform-browser-dynamic": "~12.2",
    "@angular/platform-server": "~12.2",
    "@angular/router": "~12.2",
    "@angular/upgrade": "~12.2",
    "core-js": "~2.6",
    "rxjs": "~7.4",
    "zone.js": "~0.11"
  },
  "devDependencies": {
    "@ngtools/webpack": "~12.2",
    "@types/core-js": "~2.5",
    "@types/node": "~10.12",
    "angular-router-loader": "~0.8",
    "angular2-template-loader": "~0.6",
    "awesome-typescript-loader": "~5.2",
    "ts-loader": "~9.2",
    "babel-core": "~6.26",
    "babel-loader": "~8.2",
    "babel-preset-env": "~1.7",
    "clean-webpack-plugin": "~4.0",
    "cross-env": "~5.2",
    "css-loader": "~6.4",
    "cssnano": "~4.1",
    "del-cli": "~1.1",
    "html-loader": "~0.5",
    "html-webpack-plugin": "~5.5",
    "lite-server": "~2.4",
    "node-sass": "~6.0",
    "css-minimizer-webpack-plugin": "~3.1",
    "sass-loader": "~12.2",
    "style-loader": "~0.23",
    "to-string-loader": "~1.1",
    "typescript": "~4.2",
    "terser-webpack-plugin": "~5.2",
    "webpack": "~5.60.0",
    "webpack-cli": "~4.9",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "~4.2"
  }
}
